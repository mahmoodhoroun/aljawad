        <record id="base.res_company_rule_employee" model="ir.rule">
            <field name="domain_force">['|', ('id','in', company_ids), ('id','in', user.transfer_allowed_company_ids.ids)]</field>
        </record>

        <record id="stock.stock_location_comp_rule" model="ir.rule">
            <field name="domain_force">[('company_id', 'in', user.transfer_allowed_company_ids.ids + [False])]</field>
        </record>



